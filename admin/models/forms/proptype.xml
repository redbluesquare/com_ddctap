<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset name="">
		<field
			name="ddcbookit_proptype_id"
			type="text"
			label="COM_DDCBOOKIT_PROPTYPE_ID_LABEL"
			description="COM_DDCBOOKIT_PROPTYPE_ID_DESC"
			readonly="true"
		/>
		<field
			name="proptype_title"
			type="text"
			default=""
			label="COM_DDCBOOKIT_PROPTYPE_TITLE_LABEL"
			description="COM_DDCBOOKIT_PROPTYPE_TITLE_DESC"
		/>
		<field
			name="proptype_description"
			type="editor"
			label="COM_DDCBOOKIT_PROPTYPE_DESCRIPTION_LABEL"
			description="COM_DDCBOOKIT_PROPTYPE_DESCRIPTION_DESC"
			filter="safehtml"
		/>
		<field name="state" type="list" label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC" class="chzn-color-state"
			filter="intval" size="1" default="1"
		>
			<option value="1">
				JPUBLISHED</option>
			<option value="0">
				JUNPUBLISHED</option>
			<option value="2">
				JARCHIVED</option>
			<option value="-2">
				JTRASHED</option>
		</field>
		<field
			name="table"
			type="text"
			label="COM_DDCBOOKIT_PROPTYPE_TABLE_LABEL"
			description="COM_DDCBOOKIT_PROPTYPE_TABLE_DESC"
			hidden="true"
			class="hidden"
			default="proptypes"
		/>
	</fieldset>
</form>
